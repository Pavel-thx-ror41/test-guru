<tr>
  <td><%= test.author.email %></td>
  <td><%= test.category.title %></td>
  <td><%= test.published %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td class="sort-by-text"><%= test.title %></td>
  <td><%= test.info %></td>
  <td><%= link_to t('.show'), admin_test_path(test), class: :button %>
      <%= link_to t('.edit'), edit_admin_test_path(test), class: :button %>
      <%= button_to t('.delete'), admin_test_path(test), method: :delete,
                    data: { confirm: t('helpers.submit.confirm') } %></td>
</tr>
