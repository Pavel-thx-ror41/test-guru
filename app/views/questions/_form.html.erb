      <%= form_with model: [@test, @question], local: true do |form| %>
        <div>
          <%= form.label :title %>
          <%= form.text_field :title %>
        </div>
        <div>
          <%= form.label :info %>
          <%= form.text_area :info %>
        </div>
        <div>
          <%= form.submit %>
        </div>
      <% end %>
